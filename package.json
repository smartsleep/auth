{
  "name": "auth",
  "version": "1.0.0",
  "description": "SmartSleep authentication service",
  "main": "index.js",
  "scripts": {
    "authorize": "CONFIG_ENV=production APP_ID=auth LOG_LEVEL=info npx ts-node bin/auth-client.ts",
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec 'node -r ts-node/register --inspect-brk' server/index.ts | pino-pretty",
    "test": "mocha test/**/*.ts --exit",
    "test:debug": "mocha --inspect-brk test/**/*.ts --exit"
  },
  "dependencies": {
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^6.2.0",
    "express": "^4.17.3",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.13",
    "minimist": "^1.2.0",
    "mongoose": "^5.6.4",
    "morgan": "^1.9.1",
    "ordered-config": "^0.2.0",
    "pino": "^5.12.6",
    "sleep-promise": "^8.0.1",
    "swagger-express-middleware": "^1.1.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.17.0",
    "@types/jwt-simple": "^0.5.33",
    "@types/mocha": "^5.2.7",
    "@types/pino": "^5.20.0",
    "chai": "^4.2.0",
    "mocha": "^5.0.5",
    "nodemon": "^1.19.1",
    "pino-pretty": "^2.6.1",
    "shelljs": "^0.8.3",
    "supertest": "^3.4.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "author": "Anders Borch <anders@cyborch.com> (https://github.com/cyborch)"
}
